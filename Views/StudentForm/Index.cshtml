            <div class="clear"></div>`;

        objTo_education.appendChild(educationDivAdd)
    }
    function remove_education_fields(rid) {
        $('.form-group' + rid).remove();
    }
    function resetEducation() {
        $('#courseName1').val('0');
        $('#institutionName1').val('');
        $('#academicGradingName1').val('0');
        $('#marks1').val('');
        $('#completionDate1').val('');

        for (eduRemove = 2; eduRemove <= educationAddMore; eduRemove++) {
            $('#educationData .form-group' + eduRemove).remove();
        }


    }

    function AddStudentEducation() {

        var dataEdu = [];

        //checking the dynamic input field
        for (eduD = 0; eduD <= educationAddMore; eduD++) {

            //unique input field finder
            $('#educationData .form-group' + eduD).each(function () {
                var degreeNameE = $(this).find('#courseName' + eduD).val();
                var institutionNameE = $(this).find('#institutionName' + eduD).val();
                var academicGradingE = $(this).find('#academicGradingName' + eduD).val();
                var marksE = $(this).find('#marks' + eduD).val();
                var dateOfCompletionE = $(this).find('#completionDate' + eduD).val();

                // Validate if any field is array empty
                if (degreeNameE === "" || institutionNameE === "" || academicGradingE === "" || marksE === "" || dateOfCompletionE === "") {
                    toastr.error("Please fill out all the fields for education" + (eduD + 1));
                    dataEdu = [];
                    return false; // Skip adding this data to the array
                    $('#institutionName1').focus();
                }

                // Push data to array
                dataEdu.push({
                    pdID: personalDetailId,
                    degreeName: degreeNameE,
                    institutionName: institutionNameE,
                    academicGrading: academicGradingE,
                    marks: marksE,
                    dateOfCompletion: dateOfCompletionE,
                });
            });
        }
        if (dataEdu !== null && dataEdu.length > 0) {
            var obj = {
                eduArrData: dataEdu,
            };

            $.ajax({
                url: "/StudentForm/AddStudentEducation",
                type: 'Post',
                dataType: 'Json',
                data: obj,
                success: function (data) {
                    toastr.success("Insert Sucessfully");
                    resetEducation();
                    //$('#institutionName1').focus();
                }
            });
        }
    }
    //end education 

</script>
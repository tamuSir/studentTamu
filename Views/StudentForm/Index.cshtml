        <label>To date </label> 
        <input type="date" class="form-control eToDate" id="toDate`+experienceAddMore+`" name="toDate[]" value="" placeholder="to date">
        </div>
        </div>
        <div class="clear"></div>`;

        objTo_experience.appendChild(experienceDivAdd)
    }
    function remove_experience_fields(rid) {
        $('.form-group' + rid).remove();
    }
    function resetExperience() {
        $('#companyName1').val('');
        $('#position1').val('');
        $('#fromDate1').val('');
        $('#toDate1').val('');

        for (expRemove = 2; expRemove <= experienceAddMore; expRemove++) {
            $('#experienceData .form-group' + expRemove).remove();
        }

    }

    function AddStudentExperience() {

                var dataExp = [];

                //checking the dynamic input field
                for (expD = 0; expD <= experienceAddMore; expD++) {

                //unique input field finder
                    $('#experienceData .form-group' + expD).each(function () {
                        var orgName = $(this).find('#companyName' + expD).val();
                        var position = $(this).find('#position' + expD).val();
                        var fromDate = $(this).find('#fromDate' + expD).val();
                        var toDate = $(this).find('#toDate' + expD).val();

                // Validate if any field is array empty
                if (orgName === "" || position === "" || fromDate === "") {
                    toastr.error("Please fill out all the fields for experience" + (expD + 1));
                    dataExp = [];
                    return false; // Skip adding this data to the array
                    $('#companyName1').focus();
                }

                        // Push data to array
                        dataExp.push({
                            pdID: personalDetailId,
                            orgName: orgName,
                            position: position,
                            fromDate: fromDate,
                            toDate: toDate,
                        });
                    });
                }
            if (dataExp != null && dataExp.length > 0) {
            var obj = {
                    expArrData: dataExp,
                };

                $.ajax({
                    url: "/StudentForm/AddStudentExperience",
                    type: 'Post',
                    dataType: 'Json',
                    data: obj,
                    success: function (data) {
                        toastr.success("Insert Sucessfully");
                        resetExperience();
                    //$('#companyName1').focus();
                    }
                });
            }
        }
    //end experience all 



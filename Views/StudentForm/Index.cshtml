                            <label class="form-label">Academic Grading Name</label>
                                <select id="academicGradingName` + educationAddMore + `" class="form-select">
                                <option value="">Select</option>
                                <option value="percentage">Percentage</option>
                                <option value="gpa">GPA</option>
                                <option value="cgpa">CGPA</option>
                            </select>
                        </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4 mb-3">
                                <label class="form-label">Marks</label>
                                    <input type="text" class="form-control" id="marks` + educationAddMore + `" >
                            </div>
                            <div class="col-md-4 mb-3">
                                <label class="form-label">Date of Completion</label>
                                    <input type="date" class="form-control" id="completionDate` + educationAddMore + `">

                            </div>

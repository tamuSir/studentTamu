<script type="text/javascript">

    $(document).ready(function () {
        $('#firstName').focus();
    });

    $(function () {
        document.title = 'Student Form';

    });

    var personalDetailId;

    function resetStudentInfo() {
        $('#firstName').val('');
        $('#middleName').val('');
        $('#lastName').val('');
        $('#gender').val('0');
        $('#address').val('');
        $('#dateOfBirth').val('');
        $('#telephone').val('');
        $('#phone').val('');
        $('#maritalStatus').val('0');
        $('#email').val('');
    }

    function AddStudentInfo() {
        if ($("#firstName").val() == null || $("#firstName").val() == '') {
            toastr.error("Choose First Name");
            $("#firstName").focus();
        }
        else if ($("#lastName").val() == null || $("#lastName").val() == '') {
            toastr.error("Choose last Name");
            $("#lastName").focus();
        }
        else if ($("#email").val() == null || $("#email").val() == '') {
            toastr.error("Choose email");
            $("#email").focus();
        }
        else if ($("#phone").val() == null || $("#phone").val() == '') {
            toastr.error("Choose phone");
            $("#phone").focus();
        }

        else {

            var data = {
                firstName: $('#firstName').val(),
                lastName: $('#lastName').val(),
                gender: $('#gender').val(),
                address: $('#address').val(),
                dateOfBirth: $('#dateOfBirth').val(),
                telephone: $("#telephone").val(),
                phone: $("#phone").val(),
                martialStatus: $("#maritalStatus").val(),
                email: $("#email").val()

            };
            $.ajax({
                url: "/StudentForm/AddPersonalDetailStudent",
                type: 'Post',
                dataType: 'Json',
                data: data,
                success: function (data) {                      
                    
                    if (data.code === "000") {
                        toastr.success(data.message, "Success", { timeOut: 5000 });
                        personalDetailId = data.data;
                        AddStudentEducation();
                        AddStudentExperience();
                        AddStudentEnglishEvidence();
                        $("#firstName").focus();
                        resetStudentInfo();
                    }
                    else {
                        toastr.error('Information is not save');
                    }
                }
            });
        }
    }

    function resetEnglishEvidence() {
        $('#englishTest').val('0');
        $('#testName').val('');
        $('#speaking').val('');
        $('#listening').val('');
        $('#reading').val('');
        $('#writing').val('');
        $('#overall').val('');
        $('#dateOfTest').val('');
    }

    function AddStudentEnglishEvidence() {
        if ($("#englishTest").val() == null || $("#englishTest").val() == '') {
            toastr.error("Choose english test Name");
            $("#englishTest").focus();
        }
        else if ($("#testName").val() == null || $("#testName").val() == '') {
            toastr.error("Choose Test Name");
            $("#testName").focus();
        }
        if ($("#overall").val() == null || $("#overall").val() == '') {
            toastr.error("Choose english test OverAll");
            $("#overall").focus();
        }

        else {

            var data = {
                pdID: personalDetailId,
                englishTest: $('#englishTest').val(),
                testName: $('#testName').val(),
                speaking: $('#speaking').val(),
                listening: $('#listening').val(),
                reading: $('#reading').val(),
                writing: $("#writing").val(),
                overall: $("#overall").val(),
                dateOfTest: $("#dateOfTest").val()

            };
            $.ajax({
                url: "/StudentForm/AddStudentEnglishEvidence",
                type: 'Post',
                dataType: 'Json',
                data: data,
                success: function (data) {
                    toastr.success("Insert Sucessfully");
                    resetEnglishEvidence();
                }
            });
        }
    }

